<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="project.model.Inscriere"
           table="Inscriere">
        <id name="Id" column="id">
            <generator class="increment"/>
        </id>
        <property
                name="participant"
                column="Participant"/>
        <property
                name="proba"
                column="Proba"/>
    </class>
</hibernate-mapping>